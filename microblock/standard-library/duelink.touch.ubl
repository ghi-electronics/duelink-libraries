module 'DUELink.Touch'
author 'GHI Electronics'
version 0 5 
depends DUELink 
description 'DUELink Touch Library'

  spec 'r' 'due_touch' 'Touch on pin _ charge state _ charge time _ timeout _ ms' 'auto bool auto auto' 1 true 10 '10'

to due_touch pin state time timeout {
  return ('[data:convertType]' (due_runWithResult ('_due_templateWith' 'Touch(%,%,%,%)' pin state time timeout)) 'number')
}

