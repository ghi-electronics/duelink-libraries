module 'DUELink.Temperature'
author 'GHI Electronics'
version 0 5 
depends DUELink 
description 'DUELink Temperature library'

  spec 'r' 'due_tempread' 'Read temperature on pin _ type _' 'auto auto' 18 4

to due_tempread pin type {
  return ('[data:convertType]' (due_runWithResult ('_due_templateWith' 'Temp(%,%)' pin type)) 'number')
}

