module 'DUELink.Pulse'
author 'GHI Electronics'
version 0 5 
depends DUELink 
description 'DUELink.Pulse'

  spec 'r' 'due_pulsein' 'Read pulse pin _ state _ timeout _ ms' 'auto bool auto' 1 true 1000

to due_pulsein pin state timeout {
  return ('[data:convertType]' (due_runWithResult ('_due_templateWith' 'PulseIn(%,%,%)' pin state timeout)) 'number')
}

