module 'DUELink.Rtc'
author 'GHI Electronics'
version 0 1 
depends DUELink 
description 'DUELink rtc'

  spec 'r' 'Rtc write' 'Rtc write _' 'auto' '1,2,3,4,5,6'
  spec 'r' 'Rtc read' 'Rtc read'

to 'Rtc read' {
  due_run 'dim b9[6]'
  due_run 'RtcR(b9)'
  return ('Stream read bytes' 'b9' 6)
}

to 'Rtc write' data {
  due_run ('_due_templateWith' 'RtcW([%])' data)
  return 1
}

