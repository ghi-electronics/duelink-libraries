module 'DUELink.Rtc'
author 'GHI Electronics'
version 0 5 
depends DUELink 
description 'DUELink rtc
'

  spec ' ' 'due_rtcw' 'Rtc write _' 'auto' '1,2,3,4,5,6'
  spec 'r' 'due_rtcr' 'Rtc read'

to due_rtcr {
  due_run 'dim b9[6]'
  due_run 'RtcR(b9)'
  return (due_stream_read_bytes 'b9' 6)
}

to due_rtcw data {
  due_run ('_due_templateWith' 'RtcW([%])' data)
}

