module 'DUELink.DistanceSensor'
author 'GHI Electronics'
version 0 8 
depends 'DUELink Comm' 
description 'DUELink.Distance Library'

  spec 'r' 'due_dist' 'Read distance(mm) on pulse pin _ echo pin _' 'auto auto' 3 4

to due_dist pulse echo {
  return ('[data:convertType]' (due_runWithResult ('_due_templateWith' '10*dist(%,%)' pulse echo)) 'number')
}

