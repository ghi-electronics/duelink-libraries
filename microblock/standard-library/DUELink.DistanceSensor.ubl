module 'DUELink.DistanceSensor'
author 'GHI Electronics'
version 0 4 
depends DUELink 
description 'DUELink.Distance'

  spec 'r' 'due_dist' 'Read distance pulse pin _ echo pin _' 'auto auto' 1 '10'

to due_dist pulse echo {
  return ('[data:convertType]' (due_runWithResult ('_due_templateWith' 'dist(%,%)' pulse echo)))  
}

