module 'DUELink Pulse'
author 'GHI Electronics'
version 0 12 
depends 'DUELink Daisylink' 
description 'DUELink Pulse Library'

  spec 'r' 'due_pulsein' 'read pulse pin _ charge _ (us) state _ timeout _ (ms)' 'auto num bool auto' 1 1000 true 100

to due_pulsein pin charge_us state timeout {
  return (due_returnNum (due_make_command 'PulseIn' pin charge_us state timeout))
}

